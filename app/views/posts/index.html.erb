<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Image URL</th>
      <th>Product1 URL</th>
      <th>Product2 URL</th>
      <th>Product3 URL</th>
      <th>Product4 URL</th>
      <th>Product5 URL</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.image_url %></td>
        <td><%= post.product1_url %></td>
        <td><%= post.product2_url %></td>
        <td><%= post.product3_url %></td>
        <td><%= post.product4_url %></td>
        <td><%= post.product5_url %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<div id="new-post">
  <%= link_to 'New Post', new_post_path, remote: true %>
</div>
